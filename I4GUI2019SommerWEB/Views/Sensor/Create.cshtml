@model I4GUI2019SommerWEB.Models.Sensor

@{
    ViewData["Title"] = "Create";
}

<h1>Add New Sensor To Location</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <hr />
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="SensorId"></label>
                <input asp-for="SensorId" class="form-control" />
                <span asp-validation-for="SensorId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LocationId"></label>
                <input asp-for="LocationId" class="form-control" />
                <span asp-validation-for="LocationId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TreeSort"></label>
                <input asp-for="TreeSort" class="form-control" />
                <span asp-validation-for="TreeSort" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Latitude"></label>
                <input asp-for="Latitude" class="form-control" />
                <span asp-validation-for="Latitude" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Longitude"></label>
                <input asp-for="Longitude" class="form-control" />
                <span asp-validation-for="Longitude" class="text-danger"></span>
            </div>
            <button type="submit" class="btn btn-default">Add Sensor To Location</button>
        </form>
    </div>
</div>

